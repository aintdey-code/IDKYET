local duelsSpeedEnabled = false
local duelsSpeedConnection = nil

dSyncBtn.MouseButton1Click:Connect(function()
    duelsSpeedEnabled = not duelsSpeedEnabled
    
    if duelsSpeedEnabled then
        dSyncBtn.Text = "Duels Speed: ON"
        dSyncBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
        
        if duelsSpeedConnection then
            duelsSpeedConnection:Disconnect()
        end
        
        duelsSpeedConnection = RunService.Heartbeat:Connect(function()
            local Char = player.Character
            if not Char then return end
            local HRP = Char:FindFirstChild("HumanoidRootPart")
            local Hum = Char:FindFirstChildOfClass("Humanoid")
            if not HRP or not Hum then return end
            
            local moveVector = Hum.MoveDirection
            if moveVector.Magnitude > 0.1 then
                local inputDirection = Vector3.new(moveVector.X, 0, moveVector.Z).Unit
                HRP.AssemblyLinearVelocity = Vector3.new(inputDirection.X * 59.5, HRP.AssemblyLinearVelocity.Y, inputDirection.Z * 59.5)
            else
                HRP.AssemblyLinearVelocity = Vector3.new(0, HRP.AssemblyLinearVelocity.Y, 0)
            end
        end)
    else
        dSyncBtn.Text = "Duels Speed"
        dSyncBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        
        if duelsSpeedConnection then
            duelsSpeedConnection:Disconnect()
            duelsSpeedConnection = nil
        end
        
        local Char = player.Character
        if Char and Char:FindFirstChild("HumanoidRootPart") then
            Char.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, Char.HumanoidRootPart.AssemblyLinearVelocity.Y, 0)
        end
    end
end)

local speedBoosterEnabled = false
local speedBoosterConnection = nil

speedBoosterBtn.MouseButton1Click:Connect(function()
    speedBoosterEnabled = not speedBoosterEnabled
    
    if speedBoosterEnabled then
        speedBoosterBtn.Text = "Speed booster: ON"
        speedBoosterBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
        
        if speedBoosterConnection then
            speedBoosterConnection:Disconnect()
        end
        
