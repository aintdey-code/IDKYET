    mainScrollFrame.CanvasSize = UDim2.new(0,0,0,mainLayout.AbsoluteContentSize.Y+10)
end)

local function createButton(text)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0,250,0,38)
    btn.Text = text
    btn.Font = Enum.Font.GothamMedium
    btn.TextSize = 14
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.BackgroundColor3 = Color3.fromRGB(40,40,40)
    btn.AutoButtonColor = false
    btn.BorderSizePixel = 0

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0,10)
    corner.Parent = btn

    btn.MouseEnter:Connect(function()
        if not (btn.Text:find("ON") or btn.Text:find("OFF")) then
            btn.BackgroundColor3 = Color3.fromRGB(60,60,60)
        end
    end)
    
    btn.MouseLeave:Connect(function()
        if btn.Text:find(": ON") then
            btn.BackgroundColor3 = Color3.fromRGB(0,120,215)
        else
            btn.BackgroundColor3 = Color3.fromRGB(40,40,40)
        end
    end)

    return btn
end

local speedBtn = createButton("Speed Booster: OFF")
