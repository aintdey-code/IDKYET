end)

local autoSpinEnabled = false
local autoSpinConnection = nil

paintballSpamBtn.MouseButton1Click:Connect(function()
    autoSpinEnabled = not autoSpinEnabled
    
    if autoSpinEnabled then
        paintballSpamBtn.Text = "Auto Spin: ON"
        paintballSpamBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
        
        if autoSpinConnection then
            autoSpinConnection:Disconnect()
            autoSpinConnection = nil
        end
        
        autoSpinConnection = RunService.Heartbeat:Connect(function()
            local character = player.Character
            if not character then return end
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if not humanoidRootPart then return end
            
            local SPIN_SPEED = 35
            if not humanoidRootPart:FindFirstChild("PanquakeSpin") then
                local bav = Instance.new("BodyAngularVelocity")
                bav.Name = "PanquakeSpin"
                bav.AngularVelocity = Vector3.new(0, SPIN_SPEED, 0)
                bav.MaxTorque = Vector3.new(0, 1e7, 0)
                bav.P = 1250
                bav.Parent = humanoidRootPart
            end
        end)
    else
        paintballSpamBtn.Text = "Auto Spin"
        paintballSpamBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        
        if autoSpinConnection then
            autoSpinConnection:Disconnect()
            autoSpinConnection = nil
        end
        
        local character = player.Character
        if character then
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if humanoidRootPart and humanoidRootPart:FindFirstChild("PanquakeSpin") then
                humanoidRootPart.PanquakeSpin:Destroy()
            end
        end
    end
end)

local flyDefenderEnabled = false
local flyDefenderConnection = nil
local autoBatConnection = nil

local autoBatEnabled = false
local autoBatLoopConnection = nil

autoBatBtn.MouseButton1Click:Connect(function()
    autoBatEnabled = not autoBatEnabled
    
    if autoBatEnabled then
        autoBatBtn.Text = "Auto Bat: ON"
        autoBatBtn.BackgroundColor3 = Color3.fromRGB(150, 0, 255)
        
        autoBatLoopConnection = task.spawn(function()
            while autoBatEnabled do
                local character = player.Character
                if character then
                    local humanoid = character:FindFirstChild("Humanoid")
                    if humanoid then
                        local bat = character:FindFirstChild("Bat") or player.Backpack:FindFirstChild("Bat")
                        if bat then
                            if bat.Parent == player.Backpack then
                                humanoid:EquipTool(bat)
                                task.wait(0.1)
                            end
                            local equippedBat = character:FindFirstChild("Bat")
                            if equippedBat then
                                equippedBat:Activate()
                            end
                        end
                    end
                end
                task.wait(0.15)
            end
        end)
    else
        autoBatBtn.Text = "Auto Bat"
        autoBatBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        
        if autoBatLoopConnection then
            coroutine.close(autoBatLoopConnection)
            autoBatLoopConnection = nil
        end
    end
end)

flyDefenderBtn.MouseButton1Click:Connect(function()
    flyDefenderEnabled = not flyDefenderEnabled
    
    if flyDefenderEnabled then
        flyDefenderBtn.Text = "Fly Defender: ON"
        flyDefenderBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 215)
        
        local function getNearestPlayer()
            local character = player.Character
            if not character then return nil end
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if not humanoidRootPart then return nil end
            
            local nearestPlayer = nil
            local nearestDistance = math.huge
            local myPos = humanoidRootPart.Position
            
            for _, p in pairs(Players:GetPlayers()) do
                if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
                    local distance = (p.Character.HumanoidRootPart.Position - myPos).Magnitude
                    if distance < nearestDistance then
                        nearestDistance = distance
                        nearestPlayer = p
                    end
                end
            end
            
            return nearestPlayer
        end
        
        if flyDefenderConnection then
            flyDefenderConnection:Disconnect()
        end
        
        flyDefenderConnection = RunService.Heartbeat:Connect(function()
            local character = player.Character
            if not character then return end
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            if not humanoid or not humanoidRootPart then return end
            
            local nearestPlayer = getNearestPlayer()
            if nearestPlayer and nearestPlayer.Character and nearestPlayer.Character:FindFirstChild("HumanoidRootPart") then
                local targetPos = nearestPlayer.Character.HumanoidRootPart.Position
                local direction = (targetPos - humanoidRootPart.Position).Unit
                
                local FLY_SPEED = 55
                humanoidRootPart.AssemblyLinearVelocity = direction * FLY_SPEED
                
                humanoid.PlatformStand = true
            end
        end)
        
        local batLoopActive = true
        autoBatConnection = task.spawn(function()
            while batLoopActive and flyDefenderEnabled do
                local character = player.Character
                if character then
                    local humanoid = character:FindFirstChild("Humanoid")
                    if humanoid then
                        local bat = character:FindFirstChild("Bat") or player.Backpack:FindFirstChild("Bat")
                        if bat then
                            if bat.Parent == player.Backpack then
                                humanoid:EquipTool(bat)
                                task.wait(0.1)
                            end
                            local equippedBat = character:FindFirstChild("Bat")
                            if equippedBat then
                                equippedBat:Activate()
                            end
                        end
                    end
                end
                task.wait(0.15)
            end
        end)
    else
        flyDefenderBtn.Text = "Fly Defender"
        flyDefenderBtn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        
        if flyDefenderConnection then
            flyDefenderConnection:Disconnect()
            flyDefenderConnection = nil
        end
        
        if autoBatConnection then
            coroutine.close(autoBatConnection)
            autoBatConnection = nil
        end
        
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
            
            if humanoid then
                humanoid.PlatformStand = false
            end
            
            if humanoidRootPart then
                humanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
            end
        end
    end
end)

local redDuelSpots = {
    CFrame.new(-475.51, -5.85, 25.98) * CFrame.Angles(0, math.rad(15.95), 0),
    CFrame.new(-488.59, -3.39, 24.23) * CFrame.Angles(0, math.rad(81.05), 0),
    CFrame.new(-474.57, -5.85, 26.02) * CFrame.Angles(0, math.rad(-85.51), 0),
    CFrame.new(-473.54, -5.39, 112.07) * CFrame.Angles(0, math.rad(-178.57), 0)
}

local blueDuelSpots = {
    CFrame.new(-474.76, -5.85, 92.83) * CFrame.Angles(0, math.rad(164.72), 0),
    CFrame.new(-486.61, -3.39, 93.54) * CFrame.Angles(0, math.rad(89.19), 0),
    CFrame.new(-473.64, -5.85, 93.82) * CFrame.Angles(0, math.rad(-36.21), 0),
    CFrame.new(-473.25, -5.85, 17.89) * CFrame.Angles(0, math.rad(-0.38), 0)
}

local redDuelActive = false
local blueDuelActive = false
local duelConnection = nil

local function moveToSpots(spots, color)
    local character = player.Character
    if not character then return end
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return end
    
    local spotsCompleted = 0
    
    local function moveToNextSpot()
        if spotsCompleted >= #spots then
            return false
        end
        
        local nextSpot = spots[spotsCompleted + 1]
        local distance = (nextSpot.Position - humanoidRootPart.Position).Magnitude
        
        local speed = 60
        if spotsCompleted >= 2 then
            speed = 30
        end
        
        local direction = (nextSpot.Position - humanoidRootPart.Position).Unit
        humanoidRootPart.AssemblyLinearVelocity = direction * speed
        
        local distanceThreshold = 3
        if distance <= distanceThreshold then
            spotsCompleted = spotsCompleted + 1
            humanoidRootPart.CFrame = nextSpot
            humanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
            
            if spotsCompleted < #spots then
                task.wait(0.1)
            end
        end
        
        return spotsCompleted < #spots
    end
    
    if duelConnection then
        duelConnection:Disconnect()
    end
    
    duelConnection = RunService.Heartbeat:Connect(function()
        if color == "red" and not redDuelActive then
            duelConnection:Disconnect()
            duelConnection = nil
            humanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
            return
        end
        
        if color == "blue" and not blueDuelActive then
            duelConnection:Disconnect()
            duelConnection = nil
            humanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
            return
        end
        
        local continue = moveToNextSpot()
        if not continue then
            duelConnection:Disconnect()
            duelConnection = nil
            
            if color == "red" then
                redDuelActive = false
                redDuelBtn.Text = "Red Duel"
                redDuelBtn.BackgroundColor3 = Color3.fromRGB(180, 0, 0)
            else
                blueDuelActive = false
                blueDuelBtn.Text = "Blue Duel"
                blueDuelBtn.BackgroundColor3 = Color3.fromRGB(0, 80, 180)
            end
            
            humanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        end
    end)
end

redDuelBtn.MouseButton1Click:Connect(function()
    if not redDuelActive then
        redDuelActive = true
        blueDuelActive = false
        
        redDuelBtn.Text = "Red Duel: ON"
        redDuelBtn.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        
        if blueDuelBtn.Text:find("ON") then
            blueDuelBtn.Text = "Blue Duel"
            blueDuelBtn.BackgroundColor3 = Color3.fromRGB(0, 80, 180)
        end
        
        moveToSpots(redDuelSpots, "red")
    else
        redDuelActive = false
        redDuelBtn.Text = "Red Duel"
        redDuelBtn.BackgroundColor3 = Color3.fromRGB(180, 0, 0)
        
        if duelConnection then
            duelConnection:Disconnect()
            duelConnection = nil
        end
        
        local character = player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        end
    end
end)

blueDuelBtn.MouseButton1Click:Connect(function()
    if not blueDuelActive then
        blueDuelActive = true
        redDuelActive = false
        
        blueDuelBtn.Text = "Blue Duel: ON"
        blueDuelBtn.BackgroundColor3 = Color3.fromRGB(0, 120, 255)
        
        if redDuelBtn.Text:find("ON") then
            redDuelBtn.Text = "Red Duel"
            redDuelBtn.BackgroundColor3 = Color3.fromRGB(180, 0, 0)
        end
        
        moveToSpots(blueDuelSpots, "blue")
    else
        blueDuelActive = false
        blueDuelBtn.Text = "Blue Duel"
        blueDuelBtn.BackgroundColor3 = Color3.fromRGB(0, 80, 180)
        
        if duelConnection then
            duelConnection:Disconnect()
            duelConnection = nil
        end
        
        local character = player.Character
        if character and character:FindFirstChild("HumanoidRootPart") then
            character.HumanoidRootPart.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
        end
    end
end)

